<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Profile">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="50dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView98"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp"
            android:onClick="back"
            app:srcCompat="@drawable/back" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="70dp"
            android:layout_marginTop="6dp"
            android:text="Профиль"
            android:textSize="20dp" />
    </LinearLayout>

        <ImageView
            android:id="@+id/imageView100"
            android:layout_width="match_parent"
            android:layout_height="144dp"
            android:layout_marginTop="50dp"
            app:srcCompat="@drawable/lico10" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Emmanuel Oyiboke"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="25dp" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:onClick="motorize"
            android:text="Изменить фото профиля"
            android:textAlignment="center"
            android:textColor="#48B2E7"
            android:textSize="15dp" />

        <ImageView
            android:id="@+id/imageView101"
            android:layout_width="match_parent"
            android:layout_height="81dp"
            android:layout_marginTop="10dp"
            app:srcCompat="@drawable/qrcode" />

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="405dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView22"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:text="    Имя"
                    android:textSize="20dp" />

                <EditText
                    android:id="@+id/editTextText3"
                    android:layout_width="match_parent"
                    android:layout_height="38dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="20dp"
                    android:background="@drawable/bg"
                    android:ems="10"
                    android:inputType="text"
                    android:text="   Emmanuel" />

                <TextView
                    android:id="@+id/textView23"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:text="    Фамилия"
                    android:textSize="20dp" />

                <EditText
                    android:id="@+id/editTextText4"
                    android:layout_width="match_parent"
                    android:layout_height="38dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="20dp"
                    android:background="@drawable/bg"
                    android:ems="10"
                    android:inputType="text"
                    android:text="   Oyiboke" />

                <TextView
                    android:id="@+id/textView24"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:text="    Адрес"
                    android:textSize="20dp" />

                <EditText
                    android:id="@+id/editTextTextPostalAddress"
                    android:layout_width="match_parent"
                    android:layout_height="38dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="20dp"
                    android:background="@drawable/bg"
                    android:ems="10"
                    android:inputType="textPostalAddress"
                    android:text="   Nigeria" />

                <TextView
                    android:id="@+id/textView25"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:text="    Телефон"
                    android:textSize="20dp" />

                <EditText
                    android:id="@+id/editTextPhone"
                    android:layout_width="match_parent"
                    android:layout_height="38dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="20dp"
                    android:background="@drawable/bg"
                    android:ems="10"
                    android:inputType="phone"
                    android:text="   +234 811-732-5298" />

                <EditText
                    android:id="@+id/editTextPhone2"
                    android:layout_width="match_parent"
                    android:layout_height="38dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="20dp"
                    android:background="@color/white"
                    android:ems="10"
                    android:inputType="phone" />

                <EditText
                    android:id="@+id/editTextPhone3"
                    android:layout_width="match_parent"
                    android:layout_height="38dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="20dp"
                    android:background="@color/white"
                    android:ems="10"
                    android:inputType="phone" />

                <EditText
                    android:id="@+id/editTextPhone4"
                    android:layout_width="match_parent"
                    android:layout_height="38dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginRight="20dp"
                    android:background="@color/white"
                    android:ems="10"
                    android:inputType="phone" />
            </LinearLayout>
        </ScrollView>

    </LinearLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView114"
            android:layout_width="410dp"
            android:layout_height="129dp"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/barbar" />

        <ImageView
            android:id="@+id/imageView115"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="100dp"
            android:layout_marginBottom="28dp"
            android:onClick="subrange"
            app:layout_constraintBottom_toBottomOf="@+id/imageView114"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.035"
            app:layout_constraintStart_toStartOf="@+id/imageView114"
            app:srcCompat="@drawable/heart" />

        <ImageView
            android:id="@+id/imageView126"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="153dp"
            android:layout_marginEnd="152dp"
            android:layout_marginBottom="25dp"
            android:onClick="foramina"
            app:layout_constraintBottom_toBottomOf="@+id/imageView114"
            app:layout_constraintEnd_toEndOf="@+id/imageView114"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/imageView115"
            app:layout_constraintStart_toStartOf="@+id/imageView114"
            app:srcCompat="@drawable/group321" />

        <ImageView
            android:id="@+id/imageView127"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="24dp"
            android:layout_marginBottom="28dp"
            android:onClick="home"
            app:layout_constraintBottom_toBottomOf="@+id/imageView114"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.005"
            app:layout_constraintStart_toStartOf="@+id/imageView114"
            app:srcCompat="@drawable/home23" />

        <ImageView
            android:id="@+id/imageView130"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="362dp"
            android:layout_marginEnd="17dp"
            android:layout_marginBottom="28dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView114"
            app:layout_constraintEnd_toEndOf="@+id/imageView114"
            app:layout_constraintStart_toStartOf="@+id/imageView114"
            app:srcCompat="@drawable/chelic2" />

        <ImageView
            android:id="@+id/imageView129"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="285dp"
            android:layout_marginEnd="94dp"
            android:layout_marginBottom="28dp"
            android:onClick="notification"
            app:layout_constraintBottom_toBottomOf="@+id/imageView114"
            app:layout_constraintEnd_toEndOf="@+id/imageView114"
            app:layout_constraintStart_toStartOf="@+id/imageView114"
            app:srcCompat="@drawable/kolokol" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>